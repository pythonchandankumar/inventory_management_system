{
    "version": 2,
    "builds": [
      {
        "src": "inventory_management_system/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.0" }
      },
      {
        "src": "build_files.sh"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "inventory_management_system/wsgi.py"
      }
    ]
  }